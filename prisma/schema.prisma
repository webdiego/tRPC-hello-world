generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model People {
  id        String  @id @default(cuid())
  name      String  @default("")
  message   String  @default("")
  flag      String  @default("")
  feeling   String  @default("")
  published Boolean @default(false)
}
